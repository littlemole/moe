<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

 <Import Project="..\master.props" />

 <Target Name="Build">
        <MSBuild Projects="expat\Source\lib\build.xml" Properties="Configuration=$(Configuration);Platform=$(Platform)"/>
        <MSBuild Projects="pcre/build.xml" Properties="Configuration=$(Configuration);Platform=$(Platform)"/>
		
 </Target>


 <Target Name="Clean">
        <MSBuild Projects="expat\Source\lib\build.xml" Properties="Configuration=$(Configuration);Platform=$(Platform)" Targets="Clean" />
        <MSBuild Projects="pcre/build.xml" Properties="Configuration=$(Configuration);Platform=$(Platform)" Targets="Clean" />
		
 </Target>

 <Target Name="Full">
  <Exec Command="./build.openssl.bat"/>
  <Exec Command="./scintilla.bat"/>
  <CallTarget Targets="Build"/>
 </Target>

</Project>

