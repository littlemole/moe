<?xml version="1.0"?>
<project name="native" default="release">
  <target name="release">
    <exec dir="." executable="C:\Program Files\Microsoft Visual Studio 8\Common7\IDE\devenv.exe" 
      failonerror="yes">
      <arg line="com4j.vcproj /rebuild Release"/>
    </exec>
    <exec dir="." executable="C:\Program Files\Microsoft Visual Studio 8\Common7\IDE\devenv.exe" 
      failonerror="yes">
