<?xml version="1.0" encoding="UTF-8"?>

<!--
moves imported stuf to lib  dir
-->

<project name="import" default="import" basedir=".">

 <target name="import" >

  <copy file = "moe/mol.moe.jar" toDir="../lib" /> 
  <copy file = "wsh/mol.wsh.jar" toDir="../lib" /> 
  <copy file = "io/mol.io.jar" toDir="../lib" /> 
  <copy file = "mshtml/mol.mshtml.jar" toDir="../lib" /> 
  <copy file = "office/mol.office.jar" toDir="../lib" /> 
  <copy file = "scintilla/mol.scintilla.jar" toDir="../lib" /> 
  <copy file = "setting/mol.setting.jar" toDir="../lib" /> 
  <copy file = "shellctrls/mol.shellctrl.jar" toDir="../lib" /> 
  <copy file = "typelib/mol.comtypes.jar" toDir="../lib" /> 

  <copy toDir="../src" > 
    <fileset dir="moe/">
		<include name="**/*.class"/>
	</fileset>
  </copy>

  <copy toDir="../src" > 
    <fileset dir="wsh/">
		<include name="**/*.class"/>
	</fileset>
  </copy>

  <copy toDir="../src" > 
    <fileset dir="io/">
		<include name="**/*.class"/>
	</fileset>
  </copy>

  <copy toDir="../src" > 
    <fileset dir="mshtml/">
		<include name="**/*.class"/>
	</fileset>
  </copy>  

  <copy toDir="../src" > 
    <fileset dir="office/">
		<include name="**/*.class"/>
	</fileset>
  </copy>  

  <copy toDir="../src" > 
    <fileset dir="scintilla/">
		<include name="**/*.class"/>
	</fileset>
  </copy>  

  <copy toDir="../src" > 
    <fileset dir="setting/">
		<include name="**/*.class"/>
	</fileset>
  </copy>  

  <copy toDir="../src" > 
    <fileset dir="shellctrls/">
		<include name="**/*.class"/>
	</fileset>
  </copy>  

  <copy toDir="../src" > 
    <fileset dir="typelib/">
		<include name="**/*.class"/>
	</fileset>
  </copy>

  <echo>done :-)</echo>  	
 </target>
</project> 
 